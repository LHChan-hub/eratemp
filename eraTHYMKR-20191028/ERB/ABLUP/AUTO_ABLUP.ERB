@AUTO_ABLUP
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM ABL_NUM
#DIM FIRST_LV
PRINTW 자동으로 레벨을 올립니다
REPEAT 24
    ABL_NUM = GET_ABL_NUMBER(COUNT)
    IF ABL_NUM != -1
        FIRST_LV = ABL:ABL_NUM
        CALLFORM ISABLUP{ABL_NUM}
        WHILE RESULT
            CALLFORM AUTO_ABLUP{ABL_NUM}
            SIF !RESULT
                BREAK
            CALLFORM ISABLUP{ABL_NUM}
        WEND
        SIF ABL:ABL_NUM > FIRST_LV
            PRINTFORML [%ABLNAME:ABL_NUM%] {FIRST_LV} -> {ABL:ABL_NUM}
    ENDIF
REND
PRINTW 완료되었습니다

@SHOW_AUTO_ABLUP_BUTTON
#LOCALSIZE 1
#LOCALSSIZE 1
PRINTL [101] 자동 레벨업

@IS_AUTO_ABLUP_VALUE(ARG)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
RETURNF ARG == 101
